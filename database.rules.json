{
  "rules": {
    "sessions": {
      ".read": "true",
      ".write": "true"
    },
    "users": {
      "$uid": {
        "current-session": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
        },
        "profile": {
          ".read": "true",
          ".write": "$uid === auth.uid"
        },
        "selected-calendars": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
        },
        "calendars": {
          ".read": "$uid === auth.uid",
          ".write": "false"
        },
        "tokens": {
          ".read": "false",
          ".write": "false"
        }
      }
    },
    "google-credentials": {
      ".read": false,
      ".write": false
    }
  }
}
