{
  "rules": {

    "users": {
      "$uid": {
        "currentSession": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
        },
        "email": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
        },
        "photoURL": {
          ".read": "auth !== null",
          ".write": "$uid === auth.uid"
        },
        "selectedCalendars": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
        },
        "previousSessions": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
        },
        "tokens": {
          ".read": "false",
          ".write": "false"
        }
      }
    },
    "google-credentials": {
      ".read": false,
      ".write": false
    }
  }
}
