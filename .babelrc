{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": "> 1%, last 2 versions, not ie <= 8"
      }
    }],
    "stage-2",
    "flow"
  ],
  "plugins": [
    "transform-runtime",
    "dev-expression",
    "ramda"
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
